<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- custom trace listeners -->
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="nlog" type="NLog.NLogTraceListener, NLog" />
      </listeners>
    </trace>
  </system.diagnostics>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>